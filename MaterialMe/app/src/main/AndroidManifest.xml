<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ranken.gmiller.materialmeplayground">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="edu.ranken.gmiller.materialmeplayground.DetailActivity"
            android:parentActivityName="edu.ranken.gmiller.materialmeplayground.MainActivity">
        </activity>
        <activity android:name="edu.ranken.gmiller.materialmeplayground.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>